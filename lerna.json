{
  "packages": ["packages/*"],
  "version": "independent",
  "publishConfig": {
    "access": "public"
  },
  "useWorkspaces": true,
  "publish": {
    "ignoreChanges": ["ignored-file", "*.md"],
    "message": "chore(release): publish %s",
